{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", ".next/**", "build/**"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "typecheck": {
        "dependsOn": ["^typecheck"],
        "outputs": []
      },
      "lint": {
        "dependsOn": ["^lint"],
        "outputs": []
      }
    }
  }